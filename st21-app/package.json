{
  "name": "st21-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://sadaftarannum21.github.io/st21",
  "scripts": {
    "clean": "rimraf build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "copy-assets": "npx copyfiles -u 1 './public/**/*' './build/'",
    "predeploy": "npm run clean && npm run build && npm run copy-assets",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.1",
    "@mui/icons-material": "^6.4.2",
    "@mui/lab": "^6.0.0-beta.24",
    "@mui/material": "^6.4.2",
    "@testing-library/react": "^16.2.0",
    "cra-template": "^1.2.0",
    "gh-pages": "^6.3.0",
    "react": "^19.0.0",
    "react-devtools": "^1.0.5",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.3",
    "react-scripts": "^5.0.1",
    "web-vitals": "^4.2.4"
  },
  "dependencies": {
    "react-emoji": "^0.5.0",
    "react-icons": "^5.4.0"
  }
}
